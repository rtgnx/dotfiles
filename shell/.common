# vim: ft=sh
#
# Set environment variables
export $(grep -v '^#' "$HOME/.env" | xargs)
alias $(grep -v '^#' "$HOME/.aliases" | xargs)

[ -f "$(command -v starship)" ] && eval "$(starship init $(basename $SHELL))"



if [ -f "$(command -v eza)" ]; then
  alias ls="eza"
  alias ll="eza -lrh"
  alias la="eza -larh"
fi
